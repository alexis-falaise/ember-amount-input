{
  "name": "ember-amount-input",
  "version": "5.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qonto/ember-amount-input.git"
  },
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "yarn workspace ember-amount-input run build",
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn workspaces run lint:fix",
    "prepare": "yarn build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "yarn workspace ember-amount-input run start",
    "start:test-app": "yarn workspace test-app run start",
    "test": "yarn workspaces run test"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "prettier": "^2.5.1"
  },
  "workspaces": [
    "ember-amount-input",
    "test-app"
  ],
  "engines": {
    "node": "16.* || >= 18"
  },
  "volta": {
    "node": "18.15.0",
    "yarn": "1.22.19"
  }
}
